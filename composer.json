{
    "name": "dwater/easyii",
    "version": "dev-master",
    "description": "Easy CMS based on Yii2 Framework",
    "keywords": ["easyii"],
    "type": "yii2-extension",
    "license": "GPL-3.0-only",
    "minimum-stability": "dev",
    "support": {
        "source": "https://github.com/dwater/easyii.git",
        "issues": "https://github.com/dwater/easyii/issues"
    },
    "authors": [
        {
            "name": "Ivan Vasilkov",
            "email": "noumohope@gmail.com",
            "role" : "Developer"
        },
        {
            "name": "Lance",
            "email": "dwater@126.com",
            "role" : "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "creocoder/yii2-nested-sets": "0.9.*",
        "bower-asset/fancybox": ">=3.4.0",
        "bower-asset/jquery.switcher": "*",
        "bower-asset/eonasdan-bootstrap-datetimepicker": "^4.7@dev",
        "2amigos/yii2-selectize-widget": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "yii\\easyii\\": "/"
        }
    },
    
	"config" : {
        "fxp-asset" : {
            "installer-paths" : {
            "npm-asset-library" : "vendor/npm",
            "bower-asset-library" : "vendor/bower"
            }
        }
    }
}
